<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.colorcoding.ibas</groupId>
  <artifactId>bobas.businessobjectscommon.parent</artifactId>
  <version>0.1.2</version>
  <packaging>pom</packaging>
  <name>bobas.businessobjectscommon.parent</name>
  <url>https://colorcoding.org/bobas</url>
  <organization>
    <name>Color-Coding Studio</name>
    <url>https://colorcoding.org</url>
  </organization>
  <developers>
    <developer>
      <name>Niuren Zhu</name>
      <organization>Color-Coding Studio</organization>
      <organizationUrl>https://colorcoding.org/</organizationUrl>
      <url>https://colorcoding.org/niurenzhu</url>
    </developer>
  </developers>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>  
  </properties>
  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.7.0</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.0.2</version>
        <configuration>
        </configuration>
      </plugin>
      <!--
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-source-plugin</artifactId>
        <version>3.0.1</version>
        <configuration>
          <attach>true</attach>
        </configuration>
        <executions>
          <execution>
            <phase>compile</phase>
              <goals>
                <goal>jar</goal>
              </goals>
          </execution>
        </executions>
      </plugin>
      -->
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>ibas-maven</id>
      <name>ibas maven repository</name>
      <url>http://maven.colorcoding.org/repository/maven-public/</url>
      <layout>default</layout>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.12</version>
    </dependency>
  </dependencies>
</project>
