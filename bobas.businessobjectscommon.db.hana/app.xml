<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE configuration>
<ns2:configuration xmlns:ns2="http://colorcoding.org/ibas/bobas/configuration">
    <appSettings>
        <!-- 配置项目-公司标记 -->
        <add key="Company" value="CC" />
        <!-- 配置项目-是否开启调试模式 -->
        <add key="DebugMode" value="true" />
        <!-- 配置项目-数据库类型 -->
        <add key="MasterDbType" value="HANA" />
        <!-- 配置项目-数据库地址 -->
        <add key="MasterDbServer" value="ibas-db-hana:30015" />
        <!-- 配置项目-数据库名称 -->
        <add key="MasterDbName" value="ibas_demo_v2" />
        <!-- 配置项目-数据库用户名称 -->
        <add key="MasterDbUserID" value="SYSTEM" />
        <!-- 配置项目-数据库用户密码 -->
        <add key="MasterDbUserPassword" value="Aa123456!" />
    </appSettings>
</ns2:configuration>