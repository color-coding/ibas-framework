<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE configuration>
<ns2:configuration xmlns:ns2="http://colorcoding.org/ibas/bobas/configuration">
    <appSettings>
        <!--配置项目-是否开启调试模式-->
        <add key="DebugMode" value="false" />
        <!--配置项目-序列化管理方式-->
        <add key="SerializationWay" value="jersey" />
        <!--配置项目-是否格式化输出-->
        <add key="FormattedOutput" value="true"  />
        <!--配置项目-包含对象ROOT-->
        <add key="IncludeJsonRoot" value="false"  />
    </appSettings>
</ns2:configuration>